{
  "name": "content-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "projects/content-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "projects/content-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "content:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "content:serve:production"
        },
        "ci": {
          "devServerTarget": "content:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["projects/content-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["content"]
}
